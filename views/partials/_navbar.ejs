<link rel="stylesheet" href="/css/stylesheet.css" type="text/css">
  <header>
    <nav class="nav-container">
      <div class="nav-left">
        <div class="logo">
          TINYapp
        </div>
        <div class="nav-links">
          <a href="/urls">My URLs</a>
          <%if (user) {%>
            <a href="/urls_new">Create New URL</a>
          <%} else {%>
            <a href="/login">Create New URL</a>
          <% } %>
        </div>
      </div>
      <div class="nav-button">
        <%if (user){%>
          <form method="POST" action="/logout">
              <label class="email-display"><%=user.email%></label>
            <button class="btn-signout" type="sign-out">Sign Out</button>
          </form>
          <%} else {%>
          <a href="../login">
            <button type="sign-in">Sign In</button>
          </a>
          <a href="../register">
            <button class="btn-signout" type="register">Register</button>
          </a>
          
          <%}%>
      </div>
    </nav>
  </header>